<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:ads="http://schemas.android.com/apk/res-auto"
    android:layout_marginRight="8dp"
    android:layout_marginLeft="8dp"
    android:layout_marginTop="5dp"
    android:layout_marginBottom="5dp"
    android:background="@drawable/login_trave2"
    tools:context="greendust.livecricketworldwide.Youtube.Youtube">


    <FrameLayout
        android:id="@+id/header"
        android:layout_width="match_parent"
        android:layout_marginTop="10dp"
        android:layout_height="@dimen/youtube_height">

        <com.google.android.youtube.player.YouTubePlayerView
        android:id="@+id/youtube_view"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        </com.google.android.youtube.player.YouTubePlayerView>



    </FrameLayout>




    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom"
        android:background="@color/wallet_highlighted_text_holo_dark">

        <com.google.android.gms.ads.AdView
            android:id="@+id/adView0"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            ads:adSize="BANNER"
            ads:adUnitId="ca-app-pub-2523520660707375/5393757849"
            android:layout_alignParentBottom="true"
            android:layout_centerHorizontal="true">
        </com.google.android.gms.ads.AdView>

    </FrameLayout>



</FrameLayout>
